name: "CaffeNet"
layers {
  name: "data"
  type: DATA
  top: "data"
  top: "label"
  data_param {
    source: "@YOUR_PATH_TO_DATA@/chairs_128x128_reduced/data-lmdb"
    batch_size: 128
    scale: 0.00390625
    backend: LMDB
  }
}
layers {
  name: "data_ds"
  type: DATA_AUGMENTATION
  bottom: "data"
  top: "data_ds" 
  augmentation_param {
    crop_size: 64
    max_multiplier: 1.
    recompute_mean: 0
    zoom: {
      rand_type: "uniform_bernoulli"
      mean: 0.55
      spread: 0.001
      exp: false
      prob: 1.
    }
  }
}
layers {
  name: "data_aug"
  type: DATA_AUGMENTATION
  bottom: "data_ds"
  top: "data_aug"
  top: "aug_params"
  coeff_schedule_param {
    initial_coeff: 0.
    final_coeff: 1.
    gamma: 0.00001
  }
  augmentation_param {
    crop_size: 64
    max_multiplier: 1.
    recompute_mean: 0
    zoom: {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 0.3
      exp: true
      prob: 1.
    }
    squeeze: {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 0.1
      exp: true
      prob: 1.
    }
    translate {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 0.1
      prob: 1.
    }
    rotate {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 0.2
      prob: 1.
    }
    lmult_mult: {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 0.7
      prob: 1.
      exp: true
    }
    sat_mult: {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 0.7
      prob: 1.
      exp: true
    }
    col_rotate: {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 1.5
      prob: 1.
    }
  }   
}
layers {
  name: "segm"
  type: DATA
  top: "segm"
  top: "label1"
  data_param {
    source: "@YOUR_PATH_TO_DATA@/chairs_segm_128x128_reduced/data-lmdb"
    batch_size: 128
    scale: 0.00390625
    backend: LMDB
  }
}
layers {
  name: "segm_ds"
  type: DATA_AUGMENTATION
  bottom: "segm"
  top: "segm_ds"
  augmentation_param {
    crop_size: 64
    max_multiplier: 1.
    recompute_mean: 0
    zoom: {
      rand_type: "uniform_bernoulli"
      mean: 0.55
      spread: 0.001
      exp: false
      prob: 1.
    }
  }   
}
layers {
  name: "segm_aug"
  type: DATA_AUGMENTATION
  bottom: "segm_ds"
  top: "segm_aug"
  bottom: "aug_params"
  augmentation_param {
    crop_size: 64
    max_multiplier: 1.
    recompute_mean: 0
    lmult_mult: {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 0.7
      prob: 0.
      exp: true
    }
    sat_mult: {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 0.7
      prob: 0.
      exp: true
    }
    col_rotate: {
      rand_type: "uniform_bernoulli"
      mean: 0.
      spread: 1.5
      prob: 0.
    }
  }   
}
layers {
  name: "angles"
  type: HDF5_DATA
  top: "angles"
  top: "labels"
  hdf5_data_param {
    source: "@YOUR_PATH_TO_DATA@/angles_reduced_shuffle.txt"
    batch_size: 128
  }
}
layers{
  name: "label_to_onehot"
  type: LABEL_TO_ONEHOT
  bottom: "label"
  top: "onehot"
  inner_product_param {
    num_output: 843
  }
}
layers {
  name: "fc1_label"
  type: INNER_PRODUCT_ORTH
  bottom: "onehot"
  top: "fc1_label"
  blobs_lr: 1.
  blobs_lr: 2.
  weight_decay: 0.
  weight_decay: 0. 
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
  orth_param {
    orth_method: ESMAEILI
    step: 10
    max_num_iter: 30
    eps: 0.001
    esmaeili_coeff: 0.55
    col_norm: 1
    orth_before_iter: 2
  }
}
layers {
  name: "relu1_label"
  type: RELU
  bottom: "fc1_label"
  top: "fc1_label"
}
layers {
  name: "fc2_label"
